<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxws:bindings
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
  xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  wsdlLocation="service.wsdl">

    <enableWrapperStyle>true</enableWrapperStyle>
    <enableAsyncMapping>false</enableAsyncMapping>

    <jaxws:bindings node="xsd:simpleType[@name='Type']">
    	<jaxb:typesafeEnumClass>
	        <jaxb:typesafeEnumMember value="primary" name="PRIMARY" />
	        <jaxb:typesafeEnumMember value="secondary" name="SECONDARY" />
	        <jaxb:typesafeEnumMember value="clipboard" name="CLIPBOARD" />
	    </jaxb:typesafeEnumClass>
    </jaxws:bindings>

    <jaxws:bindings node="xsd:simpleType[@name='CloudEvent_data']">
    	<jaxb:typesafeEnumClass>
	    </jaxb:typesafeEnumClass>
    </jaxws:bindings>

</jaxws:bindings>

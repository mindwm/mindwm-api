<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Description>MindWM</Description>
    <Copyright>MindWM</Copyright>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <DebugType>portable</DebugType>
    <EnableDefaultContentItems>false</EnableDefaultContentItems>
    <RunWorkingDirectory>$(MSBuildThisFileDirectory)</RunWorkingDirectory>
    <AssemblyName>MindWM</AssemblyName>
    <PackageId>MindWM</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.All" />
    <PackageReference Include="Giraffe" Version="3.4.*" />
    <PackageReference Include="TaskBuilder.fs" Version="2.1.*" />
    <PackageReference Include="AspNet.Security.OAuth.GitHub" Version="2.0.1" />
    <PackageReference Include="AspNet.Security.ApiKey.Providers" Version="1.1.0" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="openapi.yaml"/>
    <Compile Include="helpers/Helpers.fs" />
    <Compile Include="model/TmuxPaneIoDocument.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeSchema.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeRelationshipPayloadEnd.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeRelationshipPayload.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeNodePayloadAfter.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeNodePayload.fs" />
    <Compile Include="model/Neo4jCaptureDataChangePayload.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeMetaSource.fs" />
    <Compile Include="model/Neo4jCaptureDataChangeMeta.fs" />
    <Compile Include="model/Neo4jCaptureDataChange.fs" />
    <Compile Include="model/IoDocument.fs" />
    <Compile Include="model/GraphRelationshipAllOfData.fs" />
    <Compile Include="model/GraphRelationship.fs" />
    <Compile Include="model/GraphNodeAllOfData.fs" />
    <Compile Include="model/GraphNode.fs" />
    <Compile Include="model/CloudEvent.fs" />
    <Compile Include="model/ClipboardPayloadContext.fs" />
    <Compile Include="model/ClipboardPayload.fs" />
    <Compile Include="model/Clipboard.fs" />
    <Compile Include="impl/CustomHandlers.fs" />
    <Compile Include="auth/AuthSchemes.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>

</Project>